#include<stdio.h>
#include<conio.h>
void main()
{
    int a = 22;
    int b = 25;
    
    //Arithmetic Op -> +,-,*,/,%
    
    int add = a + b;
    int sub = a - b;
    int mul = a * b;
    int d1  =  a / b;
    int mod = a % b;
    
    printf("\n Your Addition is %d",add);
    printf("\n Your Substraction is %d",sub);
    printf("\n Your Multiplication is %d",mul);
    printf("\n Your Division is %d",d1);
    
    getch();
}
